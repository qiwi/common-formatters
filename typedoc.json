{
  "name": "@qiwi/common-formatters",
  "mode": "file",
  "out": "./docs",
  "target": "ES6",
  "exclude": ["test", "node_modules"],
  "externalPattern": "**/node_modules/**",
  "excludePrivate": true,
  "excludeNotExported": true,
  "moduleResolution": "node",
  "module": "commonjs",
  "hideGenerator": true,
  "readme": "./README.md",
  "ignoreCompilerErrors": true,
  "theme": "minimal"
}